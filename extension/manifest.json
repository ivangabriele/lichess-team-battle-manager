{
  "manifest_version": 3,
  "name": "Lichess Team Battle Manager",
  "version": "0.0.0",
  "description": "A simple tool to manage regular Lichess Team Battles.",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://lichess.org/inbox/*"],
  "homepage_url": "https://github.com/ivangabriele/lichess-team-battle-manager",
  "author": "Ivan Gabriele"
}
